---
import Layout from "../layouts/Layout.astro";
import ProjectLayoutLinks from "./ProjectLayout-Links.astro";
import Card from "./Card.astro";

const icons = false;
const { projects } = Astro.props;
---

<Layout title="Hirashiki Rafael's Portfolio Project" description={}>
  <ProjectLayoutLinks project={icons} />
  {
    projects.map((project: any, i:any) => (
      <>
        <Card project={project} i={i}/>
      </>
    ))
  }
</Layout>
