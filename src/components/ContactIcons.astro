---
import ProjectEmailIcon from "./ProjectEmailIcon.astro";
import ProjectGithubIcon from "./ProjectGithubIcon.astro";
import ProjectLinkedin from "./ProjectLinkedin.astro";

const { class: className, classp: classNameP} = Astro.props;

---

<div class="flex flex-col justify-center">
  <li class=`flex gap-4 mt-4 ${className}`>
    <ProjectGithubIcon url={"https://github.com/hiRafa"} />
    <ProjectLinkedin />
    <ProjectEmailIcon />
  </li>
  <p class=`pt-2 ${classNameP}`>Get in touch with me!</p>
</div>
